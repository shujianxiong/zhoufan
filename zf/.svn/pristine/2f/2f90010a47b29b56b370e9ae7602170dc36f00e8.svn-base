@import "../../baseConfig/config.less";
/**-----------------------导航栏----------------------------*/
header{
    position: fixed;
    top: 0;
    display: block;
    width: 100%;
    height: 46px;
    background: #fff;
    line-height: 46px;
    z-index: 9999;
    div{
        position: relative;
        display: inline-block;
        width: 80%;
        height: 28px;
        margin: 9px 0 9px 5%;
        border: 1px;
        .border-radius(14px) ;
        background: #dddddd;
        input{
            position: absolute;
            top: 0;
            width: 100%;
            height: 28px;
            border: 1px;
            padding-right: 40px;
            .border-radius(14px) ;
            background: url(images/search.png) no-repeat right;
            background-size: 31px;
            color: #000;
            text-indent: 10px;
            &::-webkit-input-placeholder {
                color: #F9F9F9;
            }
        }
    }
    i{
        position: absolute;
        right: 0;
        top: 0;
        font-size: 2.0rem;
        width: 10%;
        margin: 0 4%;
        text-align: center;
        color: #000000;
        span{
            position: absolute;
            top: 5px;
            right: 0;
            display: block;
            width: 20px;
            height: 20px;
            background: #ff6a56;
            .border-radius(100%);
            color: #fff;
            text-align: center;
            line-height: 20px;
        }
    }
//  span{
//      display: block;
//      float: right;
//      font-size: @commonText;
//      width: 18%;
//      color: #000;
//      text-align: center;
//  }
}
.goodsList_filter{
    position: fixed;
    top: 46px;
    height: 50px;
    width: 100%;
    background-color: @blockbacground;
    border-bottom: 1px solid @linecolor;
    z-index: 999;
    ul{
        float: left;
        width: 85%;
        height: 100%;
        li{
            float: left;
           width:30%;
           line-height: 50px;
           text-align: center;
           font-size: @commonText;
           &.active{
               color: @goldencolor;
           }
           &:nth-child(2){
          	 width:40%;
           }
           i{
               padding: 2px;
               font-size: 0.9rem;
           }
        }
    }
    span{
            float: right;
            display: inline-block;
            box-sizing: border-box;
            width: 15%;
            text-align: center;
            line-height: 50px;
            border-left: 1px solid @linecolor;
            i{
            font-size: @hintIcon !important; 
            }
        }
}
.goodsList_condent{
    position: relative;
    width: 100%;
    height: 100%;
    margin-top: 97px;
}
.goodsWrapper_vertical,.goodsWrapper_across{
	position: fixed;
    top: 97px;
    bottom: 0;
    z-index: 9;
    width: 100%;
    overflow: hidden;
}
//===============块状显示
.goodsList_vertical{
    width: 100%;
    padding: 10px 0 10px 10px;
    box-sizing: border-box;
    overflow: hidden;
    li{
        position: relative;
        float: left;
        width: 50%;
        box-sizing: border-box;
        padding: 10px 10px 0 0;
        &:last-child{
            padding: 0;
            height: 30px;
            width: 100%;
            font-size: 1.2rem;
            text-align: center;
            color: #dddddd;
            line-height: 30px;
        }
        .li_subWarp{
            position: relative;
            overflow: hidden;
            width: 100%;
            .buyMode{
                position: absolute;
                top: 0;
                left: 0;
                span{
                    display: block;
                    width: 20px;
                    text-align: center;
                    margin: 3px;
                    padding: 2px 0;
                    color: #fff;
                    line-height: 15px;
                    font-size: @describText;
                    &.saleAble{
                        background: #FF6A56;
                    }
                    &.rentAble{
                        background: #2dd6b7;
                    }
                }
            }
            img{
                width: 100%;
            }
            h2{
                line-height: 20px;
                p{
                    display: inline-block;
                    font-weight: 600;
                }
                b{
                    float: right;
                    padding-right: 10px;
                }
            }
            h3{
                overflow: hidden;
                line-height: 20px;
                height: 20px;
                width: 100%;
                box-sizing: border-box;
                padding-right: 10px;
            }
        }
    }
}
//===============横排显示
.goodsList_across{
    width: 100%;
    padding: 10px;
    overflow: hidden;
    box-sizing: border-box;
    li{
        position: relative;
        overflow: hidden;
        box-sizing: border-box;
        border: 1px solid @linecolor;
        margin-bottom: 10px;
        &:last-child{
            padding: 0;
            height: 30px;
            width: 100%;
            font-size: 1.2rem;
            text-align: center;
            color: #dddddd;
            line-height: 30px;
            border: none;
        }
        .buyMode{
            position: absolute;
            top: 0;
            right: 0;
            span{
                display: block;
                text-align: center;
                margin: 3px;
                padding: 2px 5px;
                color: #fff;
                font-size: @describText;
                &.saleAble{
                    background: #FF6A56;
                }
                &.rentAble{
                    background: #2dd6b7;
                }
            }
        }
       img{
           float: left;
           width: 35%;
           height: 100%;
           padding: 5px;
           box-sizing: border-box;
           overflow: hidden;
       }
       .price{
           float: left;
           padding: 10px;
           width: 65%;
           padding: 10px 50px 10px 10px;
           box-sizing: border-box;
           overflow: hidden;
           h3{
               overflow: hidden;
               line-height: 20px;
               height: 20px;
               width: 100%;
           }
           h2{
               position: relative;
                height: 60px;
               p{
                   position: absolute;
                   bottom: 0;
                   line-height: 30px;
                   font-weight: 600;
               }
               b{
                   position: absolute;
                   top: 0;
                   line-height: 30px; 
               }
           }
       }
    }
}
.zonghe{
    position: fixed;
    top: 97px;
    z-index: 99;
        width: 100%;
    overflow: hidden;
    background: @blockbacground;
    li{
        width: 100%;
        box-sizing: border-box;
        height: 36px;
        border-bottom: 1px solid @linecolor;
        line-height: 36px;
        padding: 0 10px;
        font-size: @viceText;
        &.active{
            color: @goldencolor;
	        i{
	            float: right;
	            display:block;
	        }
            
        }
        i{
            float: right;
            display:none;
        }
    }
}
.filtrate{
    position: fixed;
    top: 97px;
    bottom: 0;
    z-index: 99;
    width: 100%;
    overflow: hidden;
    background: @blockbacground;
    li{
        width: 100%;
        box-sizing: border-box;
        border-bottom: 1px solid @linecolor;
        padding: 10px;
        overflow: hidden;
        p{
            font-size: @viceText;
            line-height: 30px;
        }
        span{
            float: left;
            font-size: @viceText;
            padding: 5px 10px;
            margin: 5px;
            .border-radius(10px);
            background: @backgroundcolor;
            &.active{
                background: @goldencolor;
                color: #000000;
            }
        }
    }
    .button{
        overflow: hidden;
        width: 40%;
        margin: 20px auto;
        span{
            color: @goldencolor;
            font-size: @viceText;
            padding: 5px 12px;
            margin: 3px;
            border: 1px solid @goldencolor;
            .border-radius(10px);
            &#reInput{
                float: left;
            }
            &#confirm{
                float: right;
            }
        }
    }
}














