@import "../../../baseConfig/config.less";
/**-----------------------导航栏----------------------------*/
.myAgency{
    overflow: hidden;
    .top{
        height: 45px;
        padding: 10px;
        line-height: 45px;
        text-align: center;
        img{
            width: 45px;
            height: 45px;
            border-radius: 100%;
            margin: 0 10px;
        }
        span{
            line-height: 45px;
        }
    }
    .content{
        background: @blockbacground; 
        overflow: hidden;
        padding: 20px 10px;
        div{
            width: 50%;
            float: left;
            box-sizing: border-box;
            padding: 10px 20px;
            img{
                width: 100%;
            }
            h3{
                text-align: center;
                line-height: 30px;
            }
        }
    }
}
