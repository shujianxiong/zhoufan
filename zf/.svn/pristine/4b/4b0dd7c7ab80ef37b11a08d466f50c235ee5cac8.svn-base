@import "../../../../baseConfig/config.less";
.billDetail{
    overflow: hidden;
    position: fixed;
    top: -50px;
    bottom: 0;
    z-index: 9;
    width: 100%;
    overflow: hidden;
    ul{
       overflow: hidden;
        background: @blockbacground;
        li{
            width: 100%;
            padding: 10px;box-sizing: border-box;
            border-bottom: 1px solid @linecolor;
            p{
                color: #fff;
                font-size: 1.2rem;
                line-height: 20px;
            }
            h3{
                line-height: 30px;
                font-size: 1.4rem;
            }
            h4{
                color: #999;
                font-size: 1.4rem;
                line-height: 20px;
            }
        }
    }
}
