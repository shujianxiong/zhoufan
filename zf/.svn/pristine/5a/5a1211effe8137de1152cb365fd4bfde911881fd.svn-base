/**-----------------------惊声尖叫----------------------------*/
.index_content_scream{
    width: 100%;
    height: auto;
    overflow: hidden;
    ul{
        width: 100%;
        li{
            width: 100%;
            li{
                float: left;
                display: inline-block;
                width: 33.3333%;
                img{
                    width: 100%;
                    height: 100%;
                }
            }
            .active{
                 position: relative;
                 background: @goldencolor;
                 &.left:after{
                      position: absolute;
                      left: 100%;
                      border: 5px solid transparent;
                      border-left: 15px solid @goldencolor;
                      width: 0;
                      height: 0;
                      content: ' ';
                      top: 55px;
                }
                &.right:after{
                      position: absolute;
                      left: -20px;
                      border: 5px solid transparent;
                      border-right: 15px solid @goldencolor;
                      width: 0;
                      height: 0;
                      content: ' ';
                      top: 55px;
                }
                i{
                  display: block;
                  position: absolute;
                  top: 30px;
                  left: 50%;
                  margin-left: -15px;
                  text-align: center;
                  font-size: @touchIcon;
                }
                span{
                    position: absolute;
                    top: 64px;
                    display: block;
                    font-size: @firstTitle;
                    width: 100%;
                    text-align: center;
                    line-height: @commonTitle;
                    color:#2D2D2D;
                    margin-top:10px;
                }
            }
        }
    }
}

/**-----------------------小咖秀----------------------------*/
.index_content_show_content{
    width: 100%;
    height: auto;
    overflow: hidden;
    background-color: @blockbacground;
    padding: 10px;
    margin-bottom: 10px;
    box-sizing: border-box;
}
.show_content_top{
    overflow: hidden;
    width: 100%;
    i{
        color: #fff;
        font-size: @smallIcon;
    }
}
.show_content_top_pic{
    float: left;
    width: 13%;
    padding-right: 5px;
    box-sizing: border-box;
    span{
        display: block;
        .border-radius(100%);
        img{
            width:100%;
            height: 100%;
            border: 2px solid @goldencolor;
            .border-radius(100%);
            box-sizing: border-box;
        }
    }
}
.show_content_top_name{
    float: left;
    display: inline-block;
    width: 150px;
    height: 40px;
    padding-left: 10px;
    p{
        display: block;
        color: #fff;
        font-size: @commonTitle;
        line-height: 25px;
    }
    span{
        color: @goldencolor;
        font-size: @describText;
        line-height: 15px;
    }
}
.show_content_top_share{
    float: right;
    line-height: 40px;
    color: #fff;
    
}
.index_content_show_content_pic{
    overflow: auto;
    box-sizing: border-box;
    margin: 0 0 0 -5px;
    span{
        float: left;
        width: 33.333%;
        box-sizing: border-box;
        padding: 5px 0 0 5px;
        overflow: hidden;
        img{
            width: 100%;
            height: 100%;
        }
    }
}
.index_content_show_content_bottom{
    height: auto;
    overflow: hidden;
    span{
        display: block;
        float: left;
        width: 10%;
        margin: 15px 10px 0 0;
        text-align: center;
        background-color: #423954;
        .border-radius(100%);
        line-height: 35px;
        i{
            font-size: @tinyIcon;
            color: #fff;
        }
        img{
            width: 100%;
            height: 100%;
            .border-radius(100%);
        }
    }
    div{
        float: right;
        margin: 15px 10px 0 0;
        line-height: 35px;
        color: #fff;
        font-size: @viceText;
        i{
            color: #1AD7B7;
            padding: 0 5px;
            font-size: @smallIcon;
        }
    }
}
/**-----------------------预售----------------------------*/
.index_content_prepare,.index_content_prepare_product{
    width: 100%;
    height: auto;
    overflow: hidden;
}
.prepare_product_main{
    width: 100%;
    height: 150px;
    overflow: hidden;
    padding: 10px;
    box-sizing: border-box;
    background-image:url(images/product_main1.png);
    background-position: center;
    background-size: 100% ;
    background-repeat: no-repeat;
}
.prepare_product_main_right{
    width: 60%;
    height: 100%;
    padding: 5px ;
    box-sizing: border-box;
    float: right;
    .product_main_right_title{
        width: 100%;
        height: 42px;
        border-bottom: 1px solid @goldencolor;
        border-top: 1px solid @goldencolor;
        line-height: 20px;
        text-align: center;
        padding: 10px 0;
        span{
            font-size: @commonTitle;
        }
    }
    .prepare_product_main_detail{
        div{
            display: inline-block;
            width: 55%;
            padding-top: 5px;
            p{
                font-size: @describText;
                text-decoration: line-through;
                color:#75756B;
                line-height: 15px;
            }
            h3{
                font-size: @viceText;
                color: @goldencolor;
                 line-height: 15px;
            }
            h2{
                color: @goldencolor;
                font-size: @commonTitle;
                line-height: 30px;
            }
        }
        span{
            display: inline-block;
            float: right;
            .border-radius(5px);
            background-color: @goldencolor;
            color: #fff;
            height: 26px;
            text-align: center;
            line-height: 26px;
            margin: 5px 0;
            padding: 0 5px;
            font-size: @viceText;
            i{
                font-size: @tinyIcon;
            }
        }
    }
}
.product_main_progress{
    height: 30px;
    width: 100%;
    span{
        float: left;
        line-height: 30px;
        font-size: @describText;
        color: @goldencolor;
    }
}
.product_main_progress_warp{
    position: relative;
    display: inline-block;
    float: right;
    width: 60%;
    height: 10px;
    background-color: #4D4262;
    margin: 10px 0;
    .border-radius(5px)
}
.product_main_progress_bar{
    position: absolute;
    left: 0;top: 0;
    height: 10px;
    width: 50%;
    background-color: @goldencolor;
    .border-radius(5px)
}
.prepare_product_sub{
    width: 100%;
    height: auto;
    overflow: hidden;
        li{
            float: left;
            width: 32.66666%;
            height: auto;
            overflow: auto;
            margin: 3px 0 0 0;
            img{
                width: 100%;
                height: 100%;
            }
        }
        .middle_li{
            margin: 3px 1% 0 1%;
        }
}
/**-----------------------规则----------------------------*/
.index_content_rules{
    height: 100px;
    width: 100%;
    background-color: #2D2240;
    border-top: 1px solid @goldencolor;
    border-bottom: 1px solid @goldencolor;
    margin: 10px 0;
    li{
        float: left;
        height: 100%;
        width: 25%;
        padding: 10px 0;
        box-sizing:border-box;
        img{
            display: block;
            height: 60px;
            margin:0 auto;
            height: 60px;
        }
        span{
            display: block;
            line-height: 20px;
            font-size:@commonText;
            text-align: center;
        }
    }
    li.active{
            position: relative;
              background-color: @goldencolor;
              &::after{
              position: absolute;
              left: 100%;
              border: 8px solid transparent;
              border-left: 10px solid @goldencolor;
              width: 0;
              height: 0;
              content: ' ';
              top: 35px;
              }
        }
}
