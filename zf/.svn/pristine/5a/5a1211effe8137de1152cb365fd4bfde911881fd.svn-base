/**-----------------------内容区----------------------------*/
.index_content{
    width: 100%;
    margin-top: 46px;
    position: relative;
    margin-bottom: 50px;
    overflow: hidden;
}
/**-----------------------轮播图----------------------------*/
.index_viewport {
    position: relative;
    width: 100%;
    overflow: hidden;
    margin-bottom: -8px;
    .index_wrapper {
        width: 100%;
        height: 100%;
        margin: 0 auto;
    }
    
    .index_scroller {
        position: absolute;
        z-index: 1;
        -webkit-tap-highlight-color: rgba(0,0,0,0);
        height: 100%;
        -webkit-transform: translateZ(0);
        -moz-transform: translateZ(0);
        -ms-transform: translateZ(0);
        -o-transform: translateZ(0);
        transform: translateZ(0);
        -webkit-touch-callout: none;
        -webkit-user-select: none;
        -moz-user-select: none;
        -ms-user-select: none;
        user-select: none;
        -webkit-text-size-adjust: none;
        -moz-text-size-adjust: none;
        -ms-text-size-adjust: none;
        -o-text-size-adjust: none;
        text-size-adjust: none;
        background-color: #444;
    }
    .slide {
        height: 100%;
        float: left;
    }
    
    .painting {
        height: 100%;
        background-size: 100% 100%;
    }

}



#indicator {
    position: relative;
    top: -20px;
    z-index: 99;
    width: 86px;
    height: 8px;
    margin: 0 auto;
    background: url(images/bannerDot1.png) repeat;
}

#dotty {
    position: absolute;
    width: 8px;
    height: 8px;
    border-radius: 4px;
    background: #fff;
}
